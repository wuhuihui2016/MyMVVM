<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whh.mymvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyMVVM">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ListActivity" />
        <activity android:name=".activity.RealMVVMActivity" />
        <activity android:name=".activity.RoomActivity" />
        <activity android:name=".activity.InjectViewActivity" />
        <activity android:name=".activity.BindViewActivity" />
        <activity android:name=".activity.NavigationActivity" />
        <activity android:name=".activity.ViewPager2Activity" />
    </application>

</manifest>